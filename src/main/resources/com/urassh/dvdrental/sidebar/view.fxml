<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<VBox xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.urassh.dvdrental.controller.sidebar.SidebarController"
      fx:id="sidebar"
      prefWidth="300"
      stylesheets="@style.css">

    <Label fx:id="title" text="Menus" />

    <Button fx:id="rentalButton" styleClass="menu-item" text="貸し出し" />
    <Button fx:id="returnButton" styleClass="menu-item" text="返却" />
    <Button fx:id="goodsButton" styleClass="menu-item" text="商品情報" />
    <Button fx:id="membersButton" styleClass="menu-item" text="会員情報" />
</VBox>
