<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.layout.AnchorPane?>

<StackPane xmlns:fx="http://javafx.com/fxml"
           fx:controller="com.urassh.dvdrental.controller.returns.detail.ReturnDetailCellController"
           styleClass="card"
           stylesheets="@style.css">
    <AnchorPane>
        <VBox styleClass="card-content" AnchorPane.topAnchor="10" AnchorPane.leftAnchor="10">
            <Label fx:id="goodsTitleLabel" styleClass="goods-name"/>
            <Label fx:id="goodsIdLabel" styleClass="goods-id"/>
            <Label fx:id="goodsPriceLabel" styleClass="goods-price"/>
        </VBox>
    </AnchorPane>
</StackPane>
