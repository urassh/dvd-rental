<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane xmlns:fx="http://javafx.com/fxml"
           fx:controller="com.urassh.dvdrental.controller.rental.RentalCellController"
           styleClass="card"
           stylesheets="@card.css">
    <VBox styleClass="card-content">
        <Label fx:id="goodsTitleLabel" styleClass="goods-name"/>
        <Label fx:id="goodsIdLabel" styleClass="goods-id"/>
        <Label fx:id="goodsReleaseDateLabel" styleClass="goods-date"/>
    </VBox>
</StackPane>
