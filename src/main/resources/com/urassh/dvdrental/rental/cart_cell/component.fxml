<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane xmlns:fx="http://javafx.com/fxml"
           fx:controller="com.urassh.dvdrental.controller.rental.RentalCartCellController"
           styleClass="card"
           stylesheets="@card.css">
    <VBox styleClass="card-content">
        <Label fx:id="goodsTitleLabel" styleClass="goods-name"/>
        <Label fx:id="goodsIdLabel" styleClass="goods-id"/>
        <Label fx:id="goodsPriceLabel" styleClass="goods-price"/>
    </VBox>
</StackPane>
